{
    "name": "pbnl/scotoobo",
    "license": "proprietary",
    "type": "project",
    "minimum-stability":"stable",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0",
        "ext-ldap": "*",
        "composer/package-versions-deprecated": "1.11.99.5",
        "doctrine/doctrine-bundle": "^1.6||^2.0",
        "doctrine/doctrine-migrations-bundle": "^2",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/rest-bundle": "^3.0.5",
        "incenteev/composer-parameter-handler": "^2.0",
        "ninsuo/symfony-collection": "^2.1",
        "ramsey/uuid": "^3.8||^4.0",
        "sensio/framework-extra-bundle": "^6.0",
        "symfony/asset": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/expression-language": "5.3.*",
        "symfony/flex": "^1.4",
        "symfony/form": "5.3.*",
        "symfony/monolog-bundle": "^3.4",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.3.*",
        "symfony/swiftmailer-bundle": "^3.3",
        "symfony/translation": "5.3.*",
        "symfony/twig-bundle": "^5.4",
        "symfony/validator": "5.3.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^1.0||^2.0||^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "nelmio/alice": "^3.5",
        "phpunit/phpunit": "^8.4||^9.0",
        "symfony/browser-kit": "5.3.*",
        "symfony/css-selector": "5.3.*",
        "symfony/maker-bundle": "^1.13",
        "symfony/phpunit-bridge": "^3.0||^5.3",
        "symfony/stopwatch": "^5.4",
        "symfony/web-profiler-bundle": "^5.4",
        "symfony/web-server-bundle": "^4.4"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "post-install-cmd": [
            "Fuz\\Symfony\\Collection\\ScriptHandler::postInstall",
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "Fuz\\Symfony\\Collection\\ScriptHandler::postUpdate",
            "@symfony-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
